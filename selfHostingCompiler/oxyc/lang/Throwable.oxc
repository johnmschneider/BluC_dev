#include <stdbool.h>
#include <string.h>

#define Throwable_noError "NoError"

class Throwable
{
    void* data;
    char* errMsg;
    
    this(void* data, char* errMsg)
    {
        this->data = data;
        this->errMsg = errMsg;
    }
    
    void* getData()
    {
        return this->data;
    }
    
    char* getError()
    {
        return this->errMsg;
    }

    bool didntError()
    {
        return strcmp(this->errMsg, Throwable_noError) == 0;
    }
};
